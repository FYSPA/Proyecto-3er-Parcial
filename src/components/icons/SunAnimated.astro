<svg xmlns="http://www.w3.org/2000/svg" width="20" height="auto" viewBox="0 0 24 24">
  <g fill="none" stroke="#ffffff" stroke-linecap="round" stroke-width="2">
    
    <!-- Animar la línea superior vertical -->
    <path 
      d="M12 3V2" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" fill="freeze"></animate>
    </path>

    <!-- Animar la línea inferior vertical -->
    <path 
      d="M12 22v1" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="0.2s" fill="freeze"></animate>
    </path>

    <!-- Animar la línea derecha horizontal -->
    <path 
      d="M21 12h1" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="0.4s" fill="freeze"></animate>
    </path>

    <!-- Animar la línea izquierda horizontal -->
    <path 
      d="M2 12h1" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="0.6s" fill="freeze"></animate>
    </path>

    <!-- Animar diagonal superior derecha -->
    <path 
      d="M19.07 4.93l.707-.707" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="0.8s" fill="freeze"></animate>
    </path>

    <!-- Animar diagonal inferior izquierda -->
    <path 
      d="M4.93 19.07l-.707.707" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="1s" fill="freeze"></animate>
    </path>

    <!-- Animar diagonal superior izquierda -->
    <path 
      d="M4.93 4.93l.707-.707" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="1.2s" fill="freeze"></animate>
    </path>

    <!-- Animar diagonal inferior derecha -->
    <path 
      d="M19.07 19.07l.707.707" 
      stroke-dasharray="1"
      stroke-dashoffset="1"
    >
      <animate attributeName="stroke-dashoffset" from="1" to="0" dur="0.3s" begin="1.4s" fill="freeze"></animate>
    </path>

    <!-- Animar el círculo central -->
    <circle 
      cx="12" 
      cy="12" 
      r="4"
      stroke-dasharray="25.13"
      stroke-dashoffset="25.13"
    >
      <animate attributeName="stroke-dashoffset" from="25.13" to="0" dur="0.4s" begin="1.6s" fill="freeze"></animate>
    </circle>
  </g>
</svg>
